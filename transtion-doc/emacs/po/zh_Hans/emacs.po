#, no-wrap
msgid "sgml-tag-help"
msgstr ""

#. type: table
#: /source/text.texi:2046
msgid ""
"Display a description of the meaning of tag @var{tag} (@code{sgml-tag-"
"help}).  If the argument @var{tag} is empty, describe the tag at point."
msgstr ""

#. type: item
#: /source/text.texi:2047 /source/picture-xtra.texi:183
#, no-wrap
msgid "C-c /"
msgstr ""

#. type: kindex
#: /source/text.texi:2048
#, no-wrap
msgid "C-c / @r{(SGML mode)}"
msgstr ""

#. type: findex
#: /source/text.texi:2049
