#. type: subsection
#: /source/misc.texi:205
#, no-wrap
msgid "Using the Gnus Summary Buffer"
msgstr ""

#. type: Plain text
#: /source/misc.texi:208
msgid "The following commands are available in the Gnus summary buffer:"
msgstr ""

#. type: kindex
#: /source/misc.texi:210
#, no-wrap
msgid "SPC @r{(Gnus Summary mode)}"
msgstr ""

#. type: findex
#: /source/misc.texi:211
