msgstr ""

#. type: table
#: /source/calendar.texi:409
msgid "Generate a one-day calendar (@code{cal-tex-cursor-day})."
msgstr ""

#. type: item
#: /source/calendar.texi:409
#, no-wrap
msgid "t w 1"
msgstr ""

#. type: table
#: /source/calendar.texi:412
msgid ""
"Generate a one-page calendar for one week, with hours (@code{cal-tex-cursor-"
"week})."
msgstr ""

#. type: item
#: /source/calendar.texi:412
