msgstr ""

#. type: opindex
#: /source/cmdargs.texi:1207
#, no-wrap
msgid "--title"
msgstr ""

#. type: cindex
#: /source/cmdargs.texi:1208
#, no-wrap
msgid "frame title, command-line argument"
msgstr ""

#. type: table
#: /source/cmdargs.texi:1210
msgid "Specify @var{title} as the title for the initial Emacs frame."
msgstr ""

#. type: Plain text
#: /source/cmdargs.texi:1214
msgid ""
"The @samp{--name} option (@pxref{Resources}) also specifies the title for "
"the initial Emacs frame."
msgstr ""

#. type: cindex
#: /source/cmdargs.texi:1217
#, no-wrap
msgid "icons (X Window System)"
msgstr ""

#. type: cindex
#: /source/cmdargs.texi:1218
